{
    "buildCommand": "",
    "outputDirectory": ".",
    "public": true,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app/index.html"
        }
    ],
    "headers": [
        {
            "source": "/app/app.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                }
            ]
        }
    ]
}